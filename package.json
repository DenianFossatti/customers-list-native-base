{
  "name": "CustomersList",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:android": "detox build --configuration android.emu.debug",
    "build:android-release": "detox build --configuration android.emu.release",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "e2e:android": "yarn build:android && yarn test:android",
    "e2e:android-ci": "yarn build:android-release && yarn test:android-ci",
    "e2e:android-release": "yarn build:android-release && yarn test:android-release",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:ci": "eslint --fix --ext .js,.ts,.tsx --quiet",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx --fix",
    "prepare": "husky install",
    "start": "react-native start",
    "start:detox": "RN_SRC_EXT='detox.js,detox.json' react-native start --reset-cache",
    "test": "jest",
    "test:android": "detox test --configuration android.emu.debug",
    "test:android-ci": "detox test --configuration android.emu.release -l verbose --headless --record-logs all --take-screenshots all && killall node",
    "test:android-release": "detox test --configuration android.emu.release",
    "test:ci": "jest --ci",
    "test:ci:coverage": "jest --ci --coverage"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "lodash": "^4.17.21",
    "native-base": "^3.2.2",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/cli-tools": "^6.2.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/detox": "^17.14.3",
    "@types/jest": "^27.0.3",
    "@types/react-native": "^0.66.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "chalk": "^4.1.2",
    "detox": "^19.1.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "execa": "1.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "17.0.2",
    "sort-package-json": "^1.53.1",
    "typescript": "^4.5.2"
  }
}
